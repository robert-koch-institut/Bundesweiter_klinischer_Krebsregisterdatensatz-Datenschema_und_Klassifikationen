								<xs:documentation>ICD-O-3, 2003</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="32">
							<xs:annotation>
								<xs:documentation>ICD-O-3, 1. Revision 2014</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="33">
