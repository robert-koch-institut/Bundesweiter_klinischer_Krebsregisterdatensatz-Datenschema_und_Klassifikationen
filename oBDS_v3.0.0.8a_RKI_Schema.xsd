						<xs:minInclusive value="0"/>
						<xs:maxExclusive value="100000"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="DatumPSA" type="Datum_Monat_oder_Jahr_oder_Vollschaetzung_Typ" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Morphologie_ICD_O_Typ">
